{
  "name": "vue-error-boundary",
  "description": "Simple and convenient Vue error boundary, supporting vue2 and vue3.",
  "version": "1.0.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build"
  },
  "main": "./dist/index.umd.js",
